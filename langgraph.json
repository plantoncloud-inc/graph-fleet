{
  "dependencies": ["."],
  "graphs": {
    "rds_manifest_generator": "src.agents.rds_manifest_generator.graph:graph"
  },
  "env": ".env",
  "python_version": "3.11",
  "pip_config_file": "./pip.conf",
  "pip_installer": "pip",
  "dockerfile_lines": [
    "RUN apt-get update && apt-get install -y git && rm -rf /var/lib/apt/lists/*",
    "ARG BUF_USER",
    "ARG BUF_API_TOKEN",
    "ENV PIP_EXTRA_INDEX_URL=https://${BUF_USER}:${BUF_API_TOKEN}@buf.build/gen/python"
  ]
}
