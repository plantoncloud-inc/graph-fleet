{
  "dependencies": ["."],
  "graphs": {
    "aws_ecs_service": "src.agents.aws_ecs_service.graph:graph",
    "aws_ecs_troubleshooter": "src.agents.aws_ecs_troubleshooter.graph:graph",
    "rds_manifest_generator": "src.agents.rds_manifest_generator.graph:graph"
  },
  "env": ".env",
  "python_version": "3.11",
  "pip_config_file": "./pip.conf",
  "pip_installer": "pip",
  "dockerfile_lines": [
    "ENV HOME=/root NETRC=/root/.netrc PIP_NO_INPUT=1",
    "RUN mkdir -p /root && chmod 700 /root && printf \"machine buf.build\\nlogin %s\\npassword %s\\n\" \"$BUF_USER\" \"$BUF_API_TOKEN\" > /root/.netrc && chmod 600 /root/.netrc"
  ]
}
